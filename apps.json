{
  "apps":[
    {
      "name":"api",
      "script":"./apps/api.js",
      "instances":4,
      "exec_mode":"cluster",
      "env":{
        "NODE_ENV":"development"
      }
    },
    {
      "name":"scheduler",
      "script":"./apps/scheduler.js",
      "env":{
        "NODE_ENV":"development",
        "WORKER_NAME": "SCHEDULER",
        "ITERATION_INTERVAL": 5000,
        "REDIS_HOST": "127.0.0.1",
        "REDIS_PORT": 6379,
        "NOTIFICATION_QUEUE_LIST_NAME": "notifications"
      },
      "env_production":{
        "NODE_ENV":"production",
        "WORKER_NAME": "SCHEDULER",
        "ITERATION_INTERVAL": 5000,
        "REDIS_HOST": "127.0.0.1",
        "REDIS_PORT": 6379,
        "NOTIFICATION_QUEUE_LIST_NAME": "notifications"
      }
    },
    {
      "name":"messenger",
      "script":"./apps/messenger.js",
      "env":{
        "NODE_ENV":"development",
        "WORKER_NAME": "MESSENGER",
        "ITERATION_INTERVAL": 1000,
        "REDIS_HOST": "127.0.0.1",
        "REDIS_PORT": 6379,
        "NOTIFICATION_QUEUE_LIST_NAME": "notifications"
      },
      "env_production":{
        "NODE_ENV":"production",
        "WORKER_NAME": "MESSENGER",
        "ITERATION_INTERVAL": 1000,
        "REDIS_HOST": "127.0.0.1",
        "REDIS_PORT": 6379,
        "NOTIFICATION_QUEUE_LIST_NAME": "notifications"
      }
    }
  ]
}